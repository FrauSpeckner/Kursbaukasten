  <script>
   // Funktion, um den Emoji-Schneetreiben-Code zu generieren
document.getElementById('generateCode').addEventListener('click', function() {
  const imageCode = document.getElementById('imageCode').value;
  const emojiSelect = document.getElementById('emojiSelect');
  const speedSelect = document.getElementById('speedSelect');
  const emojiSize = document.getElementById('emojiSize').value;
  const generatedCode = document.getElementById('generatedCode');

  // VerfÃ¼gbare Emojis, ausgewÃ¤hltes Emoji, Geschwindigkeit und GrÃ¶ÃŸe
  const selectedOption = emojiSelect.options[emojiSelect.selectedIndex];
  const emoji = selectedOption.value;
  const speed = speedSelect.options[speedSelect.selectedIndex].value;

  // Generiere den Emoji-Schneetreiben-Code
  const code = `
    <div style="position: relative; overflow: hidden;">
      ${imageCode}
      ${Array(6)
        .fill()
        .map((item, index) => {
          const emojiSpeed = Math.random() * 25 + 5; // ZufÃ¤llige Geschwindigkeit zwischen 5 und 30 Sekunden
          const emojiRotation = Math.random() * 360; // ZufÃ¤llige Rotation zwischen 0 und 360 Grad

          return `<div class="ESG-emoji-snow" style="left: ${Math.random() * 100}%; top: -10%; animation-duration: ${emojiSpeed}s; font-size: ${emojiSize}px; animation-delay: ${Math.random() * 10}s; transform: translateY(-50vh) translateX(0) rotate(${emojiRotation}deg);">${emoji}</div>`;
        })
        .join('')}
    </div>`;

  // Zeige den generierten Code
  generatedCode.value = code;
});

// Funktion zum Kopieren des Codes
document.getElementById('copyCode').addEventListener('click', function() {
  const generatedCode = document.getElementById('generatedCode');
  generatedCode.select();
  document.execCommand('copy');
});

  </script>
</body>
</html>
        </div>
    </div>

    <div class="custom-generator-container">
        <button class="custom-generator-button" id="custom-datenbank-button">
            <span class="custom-generator-toggle-button">â–¶ï¸</span> Bilder Datenbank ğŸ–¼ï¸
        </button>
        <div class="custom-generator-content" id="custom-datenbank-content">
            <!-- Hier kann der Generator fÃ¼r die Bild-Datenbank eingefÃ¼gt werden -->
        </div>
    </div>

    <script>
        // JavaScript-Code fÃ¼r das Ein- und Ausklappen des Inhalts
        document.querySelectorAll(".custom-generator-button").forEach(function(button) {
            button.addEventListener("click", function() {
                var content = this.nextElementSibling;
                if (content.style.display === "block") {
                    content.style.display = "none";
                    this.querySelector(".custom-generator-toggle-button").textContent = "â–¶ï¸";
                } else {
                    content.style.display = "block";
                    this.querySelector(".custom-generator-toggle-button").textContent = "â–¼";
                }
            });
        });
    </script>
